<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="280.0" minWidth="280.0" prefHeight="480.0" prefWidth="540.0" styleClass="background" stylesheets="@../application/application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vista.ConPrincipal">
   <children>
      <TableView fx:id="tvContactos" layoutX="45.0" layoutY="46.0" prefHeight="341.0" prefWidth="449.0">
        <columns>
          <TableColumn fx:id="tcNombre" prefWidth="75.0" text="Nombre" />
          <TableColumn fx:id="tcApellido" prefWidth="75.0" text="Apellido" />
            <TableColumn fx:id="tcTelefono" prefWidth="75.0" text="Telefono" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnNuevo" layoutX="108.0" layoutY="413.0" mnemonicParsing="false" onAction="#nuevaPersona" text="Nuevo" />
      <Button fx:id="btnBorrar" layoutX="371.0" layoutY="413.0" mnemonicParsing="false" onAction="#borrarPersona" text="Borrar" />
      <Button fx:id="btnEditar" layoutX="238.0" layoutY="413.0" mnemonicParsing="false" onAction="#editarPersona" text="Editar" />
   </children>
</AnchorPane>
